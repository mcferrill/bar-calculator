{
    "name": "bar-new",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "build": "bun run build.ts && bun run tailwindcss -i ./src/index.css -o ./dist/index.css",
        "dev": "bun run tailwindcss -i ./src/index.css -o ./dist/index.css --watch & bun run serve.ts",
        "lint": "bun run tsc --noEmit"
    },
    "dependencies": {
        "@sentry/react": "^9.8.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwindcss": "3"
    },
    "devDependencies": {
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "autoprefixer": "^10.4.21",
        "bun-types": "^1.2.5",
        "eslint": "^9.22.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3"
    }
}
